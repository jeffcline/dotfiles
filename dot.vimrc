set nocompatible
set backspace=indent,eol,start
set nobackup
set history=100
set spelllang=en_us
set showmatch
syntax enable
set linebreak
set showbreak=+++
filetype plugin on
set iskeyword+=-
filetype indent on
set autoindent
set tabstop=4
set shiftwidth=4
set incsearch
set ignorecase
set smartcase
set ruler
set showcmd
set showmode
set wildmenu
set number
